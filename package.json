{
  "name": "@ralacerda/neverthrow2",
  "version": "0.1.0",
  "description": "Stop throwing errors, and instead return Results!",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "local-ci": "pnpm run typecheck && pnpm run lint && pnpm run test && pnpm run format && pnpm run build",
    "test": "vitest run && pnpm run test-types",
    "test-types": "tsc --noEmit -p ./tests/tsconfig.tests.json",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --write 'src/**/*.ts?(x)' && pnpm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf ./dist ./tmp",
    "build": "pnpm run clean && rollup --config && mv tmp/*.js dist && attw --pack .",
    "prepublishOnly": "pnpm run build",
    "release": "changeset publish",
    "version": "changeset version && pnpm i --lockfile-only"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ralacerda/neverthrow2.git"
  },
  "author": "Renato Lacerda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ralacerda/neverthrow2/issues"
  },
  "homepage": "https://github.com/ralacerda/neverthrow2#readme",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@types/node": "^18.19.39",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "eslint": "7.30.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "prettier": "2.2.1",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.32.1",
    "testdouble": "3.20.2",
    "ts-toolbelt": "9.6.0",
    "typescript": "4.7.2",
    "vitest": "^2.1.3"
  },
  "keywords": [
    "typescript",
    "functional",
    "fp",
    "error"
  ],
  "engines": {
    "node": ">=18"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.0"
  },
  "packageManager": "pnpm@10.13.1"
}
